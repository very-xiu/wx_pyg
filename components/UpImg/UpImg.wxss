/* components/UpImg/UpImg.wxss */
.up_img_wrap{
  width: 92rpx;
  height: 92rpx;
  position: relative;
}
.up_img_wrap image{
  width: 100%;
  height: 100%;
  border-radius: 15rpx;
  border: 1rpx solid #ccc;
}
.up_img_wrap icon{
  position: absolute;
  top: -13rpx;
  right: -13rpx;
  background-color: #fff;
  border-radius: 50%;
}